{
  "name": "pixelsnob",
  "version": "0.0.1",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "forever start -c 'node --harmony_array_includes' server/dist/app.js",
    "start-dev": "NODE_ENV=development; nodemon --harmony_array_includes server/dist/app.js  --watch server/dist",
    "build": "webpack --progress --config webpack/config/all.js",
    "build-watch": "webpack --progress --config webpack/config/all.js --watch",
    "build-client": "webpack --progress --config webpack/config/client.js",
    "build-watch-client": "webpack --progress --config webpack/config/client.js --watch",
    "build-server": "webpack --progress --config webpack/config/server.js",
    "build-watch-server": "webpack --progress --config webpack/config/server.js --watch",
    "get-client-size": "webpack --config webpack/config/client.js --json | webpack-bundle-size-analyzer"
  },
  "dependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^9.4.2",
    "node-dom": "^0.1.0",
    "pug": "^2.0.0-beta5",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.6.1",
    "server-components-express": "^0.1.0",
    "virtual-dom": "^2.1.1",
    "webcomponents.js": "^0.7.22",
    "webcomponentsjs": "^1.0.2",
    "webpack": "^1.13.2",
    "webpack-bundle-size-analyzer": "^2.0.2",
    "webpack-node-externals": "^1.3.3"
  },
  "devDependencies": {}
}
